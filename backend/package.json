{
  "name": "ai-finance-dashboard-backend",
  "version": "1.0.0",
  "description": "AI Finance Dashboard Backend API",
  "main": "dist/index.js",
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  },
  "scripts": {
    "build": "tsc",
    "watch": "tsc --watch",
    "clean": "rimraf dist",
    "start": "node dist/index.js",
    "start:prod": "node dist/index.js",
    "dev": "nodemon --exec tsx src/index.ts",
    "dev:watch": "nodemon --watch src --ext ts --exec tsx src/index.ts",
    "type-check": "tsc --noEmit",
    "migrate:deploy": "prisma migrate deploy",
    "migrate:dev": "prisma migrate dev",
    "migrate:reset": "prisma migrate reset",
    "generate": "prisma generate",
    "postinstall": "prisma generate",
    "db:studio": "prisma studio",
    "seed": "tsx prisma/seed.ts",
    "seed:prod": "prisma db seed",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:ci": "jest --ci --coverage --watchAll=false",
    "test:redis": "tsx scripts/test-redis.ts",
    "lint": "eslint . --ext .ts,.tsx --max-warnings 0",
    "lint:fix": "eslint . --ext .ts,.tsx --fix",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "render:build": "pnpm install && pnpm prisma generate && pnpm run build",
    "render:start": "pnpm prisma migrate deploy && pnpm start",
    "render:seed": "pnpm prisma migrate deploy && tsx prisma/seed.ts",
    "health": "curl -f http://localhost:3001/health || echo 'Server not running'",
    "logs": "pm2 logs --lines 50",
    "restart": "pm2 restart all"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "packageManager": "pnpm@10.15.1",
  "dependencies": {
    "@google/generative-ai": "^0.24.1",
    "@prisma/client": "6.15.0",
    "bcryptjs": "^3.0.2",
    "compression": "^1.8.1",
    "cors": "^2.8.5",
    "dotenv": "^17.2.2",
    "express": "^4.21.2",
    "helmet": "^8.1.0",
    "ioredis": "^5.7.0",
    "jsonwebtoken": "^9.0.2",
    "morgan": "^1.10.1",
    "redis": "^5.8.2",
    "ts-node": "^10.9.2",
    "zod": "^4.1.5"
  },
  "devDependencies": {
    "@types/bcryptjs": "^3.0.0",
    "@types/compression": "^1.8.1",
    "@types/cors": "^2.8.19",
    "@types/express": "^4.17.23",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/morgan": "^1.9.10",
    "@types/node": "^24.3.1",
    "@types/redis": "^4.0.11",
    "nodemon": "^3.1.10",
    "prisma": "^6.15.0",
    "rimraf": "^5.0.5",
    "tsx": "^4.20.5",
    "typescript": "^5.9.2"
  }
}
