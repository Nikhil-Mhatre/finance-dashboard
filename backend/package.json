{
  "name": "ai-finance-dashboard-backend",
  "version": "1.0.0",
  "description": "AI Finance Dashboard Backend API",
  "main": "dist/index.js",
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  },
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "dev": "nodemon --exec tsx src/index.ts",
    "dev:watch": "nodemon --watch src --ext ts --exec tsx src/index.ts",
    "clean": "rimraf dist",
    "type-check": "tsc --noEmit",
    "migrate:deploy": "prisma migrate deploy",
    "generate": "prisma generate",
    "postinstall": "prisma generate",
    "render:build": "npm install && npx prisma generate && npm run build",
    "render:start": "npx prisma migrate deploy && npm start"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "packageManager": "pnpm@10.15.1",
  "dependencies": {
    "@google/generative-ai": "^0.24.1",
    "@prisma/client": "6.15.0",
    "bcryptjs": "^3.0.2",
    "compression": "^1.8.1",
    "cors": "^2.8.5",
    "dotenv": "^17.2.2",
    "express": "^4.21.2",
    "helmet": "^8.1.0",
    "jsonwebtoken": "^9.0.2",
    "morgan": "^1.10.1",
    "zod": "^4.1.5"
  },
  "devDependencies": {
    "@types/bcryptjs": "^3.0.0",
    "@types/compression": "^1.8.1",
    "@types/cors": "^2.8.19",
    "@types/express": "^4.17.23",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/morgan": "^1.9.10",
    "@types/node": "^24.3.1",
    "nodemon": "^3.1.10",
    "prisma": "^6.15.0",
    "tsx": "^4.20.5",
    "typescript": "^5.9.2"
  }
}
